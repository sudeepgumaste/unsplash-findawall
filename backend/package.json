{
  "name": "unsplash-gallery-backend",
  "version": "0.1.0",
  "description": "backend to fetch wit unsplash-js and serve images to frontend",
  "main": "server.js",
  "scripts": {
    "watch": "nodemon --exec babel-node src/index.js",
    "build": "babel src --out-dir dist",
    "start": "babel src --out-dir dist && node dist/index.js"
